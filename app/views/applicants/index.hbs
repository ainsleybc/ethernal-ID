<div class="add_new">
  <a href="/applicants/new" class="btn-large waves-effect waves-light teal lighten-1">Add new applicants</a>
</div>


{{#each applicants }}
  <div>
    <p>Name: {{this.applicantName}}</p>
    <p>D.O.B.: {{this.applicantDob}}</p>
    <form method="post" action="/identities">
      <input type="hidden" name="applicant" value="{{this._id}}">
      <button  class="btn waves-effect waves-light" type="submit" name="action">Verify
  <i class="material-icons right">done</i>
</button>
    </form>
    <div>
{{/each}}
